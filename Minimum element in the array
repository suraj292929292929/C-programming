
#include <stdio.h>
int main(){   
    int n;
    printf("enter the no of elements present in the array: ");
    scanf("%d",&n);
    int arr[n];
    for(int i = 0;i<n;i++){
        printf("enter the %d  index elements: ",i);
        scanf("%d",&arr[i]);
    }
    int min = arr[0];
    for(int i=0;i<n;i++){
        if(arr[i]<min){
            min = arr[i];
        }
        
    }
    printf("The minimum number in the array is %d",min);    
}
