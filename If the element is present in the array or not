#include<stdio.h>
int main() {
    int n;
    printf("Enter the number of elements in the array: ");
    scanf("%d", &n);

    int arr[n];
    for (int i = 0; i < n; i++) {
        printf("Enter element at index %d: ", i);
        scanf("%d", &arr[i]);
    }
    int target;
    printf("enter the element: ");
    scanf("%d",&target);
    
    int flag = 0;
    int ind = 0;
    for(int i = 0; i < n; i++){
        if(arr[i] == target){
            flag = 1;
            ind = i;
            break;
    }
   
}
 if(flag == 1){
        printf("element is present in the array at %d index",ind);
    }
    else{
        printf("element is not present in the array");
    }
}
