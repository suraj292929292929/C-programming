#include<stdio.h>
int main(){
    int n;
    printf("enter the no of element present in the array: ");
    scanf("%d",&n);
    int arr[n];
    int brr[n];
    for(int i = 0; i < n; i++){
        printf("enter the %d index elements: ",i);
        scanf("%d",&arr[i]);
    }
    for(int i = 0; i < n; i++){
        brr[i] = arr[n-1-i];
    } 
    for(int i=0;i<n;i++){
        printf("%d",brr[i]);
    }
    
}
